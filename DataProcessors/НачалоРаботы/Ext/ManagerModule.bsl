	Для Ном = 1 По Область.ВысотаТаблицы Цикл 
		Код = СокрЛП(Область.Область(Ном, 1).Текст);
		Если Код = "112" Или Код = "417" Или Код = "156" Или Код = "498" Или Код = "643" Или Код = "762" Или Код = "795" Или Код = "860" Или Код = "804" Тогда 
